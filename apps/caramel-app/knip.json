{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "entry": [
        "src/app/**/*.{ts,tsx}",
        "src/instrumentation*.ts",
        "e2e/**/*.spec.ts",
        "scripts/*.ts"
    ],
    "project": ["src/**/*.{ts,tsx}", "e2e/**/*.ts"],
    "ignore": [
        "eslint.config.mjs",
        "postcss.config.mjs",
        "postcss.config.ts",
        "tailwind.config.ts",
        "public/sw.js",
        "src/components/StoreButtons.tsx",
        "src/lib/capitalizeFirst.ts",
        "src/lib/cors.ts",
        "src/lib/initMiddleware.ts",
        "src/lib/middlewares/**",
        "src/lib/securityHelpers/apiResponse.ts",
        "src/lib/gtag.ts",
        "src/lib/securityHelpers/cryptoHelpers.ts"
    ],
    "ignoreDependencies": [
        "@argos-ci/cli",
        "@tailwindcss/forms",
        "@types/cors",
        "@types/gtag.js",
        "@types/jsonwebtoken",
        "@typescript-eslint/parser",
        "eslint-config-next",
        "prettier-plugin-organize-imports",
        "prettier-plugin-tailwindcss",
        "@heroicons/react",
        "@use-gesture/react",
        "cors",
        "gsap",
        "jsonwebtoken",
        "react-awesome-button",
        "react-fast-marquee",
        "react-lottie-player",
        "react-type-animation"
    ],
    "ignoreBinaries": ["eslint"],
    "ignoreExportsUsedInFile": true
}
